{
  "name": "datenkraft/bb-xxx-api-php-client",
  "description": "The Xxx API PHP Client enables you to work with the Xxx API",
  "type": "library",
  "require": {
    "php": "^8.0",
    "datenkraft/bb-base-api-php-client": "^3.0.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Datenkraft GmbH",
      "email": "office@datenkraft.com"
    }
  ],
  "minimum-stability": "stable",
  "autoload": {
    "psr-4": {
      "Datenkraft\\Backbone\\Client\\XxxApi\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.6",
    "pact-foundation/pact-php": "^7.1",
    "squizlabs/php_codesniffer": "^3.7",
    "jane-php/open-api-3": "^7.5.0",
    "friendsofphp/php-cs-fixer": "^3.16"
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    },
    "platform": {
      "php": "8.0"
    }
  }
}
